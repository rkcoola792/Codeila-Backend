<h1>Codeial/<%=title%></h1>

<section id="feed-post">
  <h4>Posts</h4>
  <form action="/posts/create" id="new-form-post" method="Post">
    <textarea
      name="content"
      id=""
      cols="30"
      rows="3"
      placeholder="Type Here"
    ></textarea>
    <input type="submit" value="Post" />
  </form>

  <div class="post-list-container">
    <ul>
      <%for(post of posts){%>

      <li>
        <p><%=post.content%></p>
      </li>
      <p><%=post.user.name%> <%}%></p>
    </ul>
  </div>
</section>
